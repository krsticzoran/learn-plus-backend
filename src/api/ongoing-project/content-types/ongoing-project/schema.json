{
  "kind": "collectionType",
  "collectionName": "ongoing_projects",
  "info": {
    "singularName": "ongoing-project",
    "pluralName": "ongoing-projects",
    "displayName": "ongoingProject"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "projectCode": {
      "type": "string"
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date",
      "required": true
    },
    "coordinator": {
      "type": "string",
      "required": true
    },
    "partners": {
      "type": "component",
      "component": "partner.partner",
      "repeatable": true,
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
